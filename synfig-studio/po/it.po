msgstr "Aggiungi Nuovo Fotogramma Chiave"

#: ../src/gui/actionmanagers/keyframeactionmanager.cpp:244
msgid "Keyframe Properties"
msgstr "Proprietà Fotogramma Chiave"

#: ../src/gui/actionmanagers/keyframeactionmanager.cpp:248
msgid "Toggle Keyframe"
msgstr "Alterna Fotogramma Chiave"

#: ../src/gui/actionmanagers/keyframeactionmanager.cpp:252
#: ../src/gui/canvasview.cpp:3347
msgid "Set Keyframe Description"
msgstr "Imposta Descrizione Fotogramma Chiave"

#: ../src/gui/actionmanagers/layeractionmanager.cpp:108
#: ../src/gui/actionmanagers/layeractionmanager.cpp:269
#: ../src/gui/actionmanagers/layeractionmanager.cpp:523
msgid "Increase Amount"
msgstr "Incrementa Valore"

#: ../src/gui/actionmanagers/layeractionmanager.cpp:120
#: ../src/gui/actionmanagers/layeractionmanager.cpp:270
